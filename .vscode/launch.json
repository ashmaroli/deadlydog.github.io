	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "PowerShell Launch Current File",
			"type": "PowerShell",
			"request": "launch",
			"script": "${file}",
			"cwd": "${file}"
		},
		{
			"name": "Start Jekyll in PowerShell Integrated Console",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/.vscode/StartJekyllServe.ps1",
			"args": ["${workspaceFolder}"],
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Close all open staticman comment pull requests",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/Scripts/CloseAllOpenStaticmanPullRequests.ps1",
			"cwd": "${workspaceFolder}"
		}
	]
}
